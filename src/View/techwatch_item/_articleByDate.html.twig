<div class = 'carouselHome'>
    {% for article in articleDate %}
        <div class="card" style="width: 18rem;">
            <a href="{{ article.article_url }}">
            <img src="{{ article.image_url }}" class="card-img-top" alt="image random">
            <div class="card-body">
                <h5 class="card-title">{{ article.title }}</h5>
            </div>
            </a>
        </div>
    {% endfor %}
</div>